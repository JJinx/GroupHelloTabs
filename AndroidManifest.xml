<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.grouphelloworld"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.grouphelloworld.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.grouphelloworld.RyanTab"
            android:label="@string/title_activity_ryan_tab" >
        </activity>
        <activity
            android:name="com.example.grouphelloworld.RobTab"
            android:label="@string/title_activity_rob_tab" >
        </activity>
        <activity
            android:name="com.example.grouphelloworld.RachelTab"
            android:label="@string/title_activity_rachel_tab" >
        </activity>
        <activity
            android:name="com.example.grouphelloworld.BobbyTab"
            android:label="@string/title_activity_bobby_tab" >
        </activity>
    </application>

</manifest>
